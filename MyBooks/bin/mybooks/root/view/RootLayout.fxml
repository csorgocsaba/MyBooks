<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="650.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="mybooks.root.MainApp">
   <top>
      <MenuBar fx:id="menuBar" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Book">
            <items>
              <MenuItem mnemonicParsing="false" text="List" onAction="#showList" />
              <MenuItem mnemonicParsing="false" text="Import" onAction="#showImport" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
